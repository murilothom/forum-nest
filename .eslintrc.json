{
  "extends": "@rocketseat/eslint-config/node",
  "rules": {
    "no-useless-constructor": "off",
    "@typescript-eslint/no-unused-vars": "warn",
    "no-new": "off",
    "max-len": [
      "error",
      {
        "code": 170,
        "tabWidth": 2,
        "ignoreUrls": true,
        "ignoreTemplateLiterals": true,
        "ignoreRegExpLiterals": true
      }
    ]
  }
}